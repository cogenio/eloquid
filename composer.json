{
    "name": "datakrama/eloquid",
    "description": "Eloquid - Auto UUID for Eloquent Model",
    "type": "library",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "laravel/legacy-factories": "^1.0.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmad Husen",
            "email": "hi@husen.id"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Datakrama\\Eloquid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datakrama\\Eloquid\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
